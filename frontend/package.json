    "next": "^14.2.12",
    "react": "^18.3.1",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "react-leaflet-routing-machine": "^0.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
